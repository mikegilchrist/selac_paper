@Article{LopezCampistrousEtAl2005,
  Title                    = {Localization, annotation, and comparison of the \emph{{E}scherichia coli} K-12 proteome under two states of growth},
  Author                   = {Lopez-Campistrous, A. and Semchuk, P. and Burke, L. and Palmer-Stone, T. and Brokx, S. J. and Broderick, G. and Bottorff, D. and Bolch, S. and Weiner, J. H. and Ellison, M. J.},
  Journal                  = {Molecular \& Cellular Proteomics},
  Year                     = {2005},
