@Article{BremermannAndPickering1983,
  Title                    = {A game-theoretical model of parasite virulence},
  Author                   = {Bremermann, Hans J. and Pickering, John},
  Journal                  = {Journal of Theoretical Biology},
  Year                     = {1983},
  Pages                    = {411-426},
  Volume                   = {100},

  Annote                   = {Game theory approach. Look at Nash-equilibrium. Focus is on evolution of virulence within the context of single and multiple infections. In intro they cite some interesting sounding studies looking at variation in pathogenicity (i.e. viruence) in various biological systems. Mention that one way to test whether or not a pathogen is at its max virulence (i.e. cant evolve to be more virulent even if selection was for greater levels) is through serial passages.Stairs 1973 = good survey of \emph{Baculovirus} virulence variation. Shapiro and Ignoffo 1970 for variation in of same genus on the cotton bollworm. Fenner and Ratcliffe, 1965 for coevolution of both myxoma and rabbits. B\&P's findings: When nash equil. exists there can be coexistance of multiple parasites, if it doesn't exist then you get contiunual invasion by ever more virulent parasites. " For a broad range of infections diseases it can be assumed that the transmission trate and the disease induced mortality rate depend on the reproductive rate ($\lambda$) of the parasites, such that $\beta(\lambda)$ and $\alpha(\lambda)$ are monotonic increasing function of $\lambda$." Here reproduction apparently means the production of transmission propagules. "More generally, increases in host mortality with the size of parasitic infection are well documented from laboratory studies." no refs. B\&P functionally link $\alpha$ to $\lambda$ -- should cite for this. Assume that transmission is proportional to reproductive output of parasite, i.e. $\beta \propto \beta_e \lambda$. The implicitly ignore replication of parasite population within a host. Assume that $\alpha(\lambda)$ is convex upward, i.e. accelerating and use a quadratic function (but also mention more complex case where its an infinite polynomial which should describe any function). Come up with an optimal birth rate which will be infinite if the term if the relationship is linear. Also mention that in general if $\nu$ is 0 then it will select for less virulent parasites. (i.e. recovery becomes independent of $\alpha$, imagining, I assume, a step function of some sort) Competing strains within a host: Assuming interactions between parasite effects (i.e. parasite damage is not additive across strains) then $\alpha = (\sum_{i=1}^n c_i \lambda_i)^2$ then there is no nash equilibrium and selection favors ever increasing parsite reproductive rates. This makes since since due to the non-additivities of strain added mortality. Looking at additive case and others they find that if the exponent is greater than 2, presumably, then you can have a nash equilibrium iff the exponent term is greater than the number of competeting strains.}
}

@Article{BrennerEtAl2000,
  Title                    = {Gene expression analysis by massively parallel signature sequencing (MPSS) on microbead arrays},
  Author                   = {Brenner, S. and Johnson, M. and Bridgham, J. and Golda, G. and Lloyd, D. H. and Johnson, D. and Luo, S. J. and McCurdy, S. and Foy, M. and Ewan, M. and Roth, R. and George, D. and Eletr, S. and Albrecht, G. and Vermaas, E. and Williams, S. R. and Moon, K. and Burcham, T. and Pallas, M. and DuBridge, R. B. and Kirchner, J. and Fearon, K. and Mao, J. and Corcoran, K.},
  Journal                  = {Nature Biotechnology},
  Year                     = {2000},
  Pages                    = {630-634},
  Volume                   = {18},

  Abstract                 = {We describe a novel sequencing approach that combines non-gel-based signature sequencing with \emph{in vitro} cloning of millions of templates on separate 5 mu m diameter microbeads. After constructing a microbead library of DNA templates by \emph{in vitro} cloning, we assembled a planar array of a million template-containing microbeads in a flow cell at a density greater than 3 x 10(6) microbeads/cm(2). Sequences of the free ends of the cloned templates on each microbead were then simultaneously analyzed using a fluorescence-based signature sequencing method that does not require DNA fragment separation. Signature sequences of 19-20 bases were obtained by repeated cycles of enzymatic cleavage with a type Ils restriction endonuclease, adaptor ligation, and sequence interrogation by encoded hybridization probes. The approach was validated by sequencing over 269,000 signatures from two cDNA libraries constructed from a fully sequenced strain of Saccharomyces cerevisiae, and by measuring gene expression levels in the human cell line THP-1. The approach provides an unprecedented depth of analysis permitting application of powerful statistical techniques for discovery of functional relationships among genes, whether known or unknown beforehand, or whether expressed at high or very low levels.},
  File                     = {Brenner.et.al.00.pdf:/home/mikeg/References/Brenner.et.al.00.pdf:PDF}
}

