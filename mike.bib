  Keywords                 = {ACYL CARRIER PROTEIN; FATTY-ACID BIOSYNTHESIS; STREPTOCOCCUS-PNEUMONIAE; PSEUDOMONAS-AERUGINOSA; ANTIBIOTIC-ACTIVITY; ENZYMATIC-SYNTHESIS; DEACETYLASE LPXC; GENE-EXPRESSION; A BIOSYNTHESIS; COENZYME-A},
  Timestamp                = {2011.1.6}
}

