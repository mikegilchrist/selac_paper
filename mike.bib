@Article{TjadenEtAl2002,
  Title                    = {Transcriptome analysis of \emph{{E}scherichia coli} using high-density oligonucleotide probe arrays},
  Author                   = {Tjaden, B. and Saxena, R. M. and Stolyar, S. and Haynor, D. R. and Kolker, E. and Rosenow, C.},
  Journal                  = {Nucleic Acids Research},
  Year                     = {2002},
  Pages                    = {3732-3738},
  Volume                   = {30},

  Abstract                 = {Microarrays traditionally have been used to analyze the expression behavior of large numbers of coding transcripts. Here we present a comprehensive approach for high-throughput transcript discovery in \emph{Escherichia coli} focused mainly on intergenic regions which, together with analysis of coding transcripts, provides us with a more complete insight into the organism's transcriptome. Using a whole genome array, we detected expression for 4052 coding transcripts and identified 1102 additional transcripts in the intergenic regions of the E.coli genome. Further classification reveals 317 novel transcripts with unknown function. Our results show that, despite sophisticated approaches to genome annotation, many cellular transcripts remain unidentified. Through the experimental identification of all RNAs expressed under a specific condition, we gain a more thorough understanding of all cellular processes. }
}

@Article{TobiasEtAl1991,
  Title                    = {THE N-END RULE IN BACTERIA},
