}

@Article{QuinnEtAl2000,
  Title                    = {Viral load and heterosexual transmission of human immunodeficiency virus type 1},
  Author                   = {Quinn, T. C. and Wawer, M. J. and Sewankambo, N. and Serwadda, D. and Li, C. J. and Wabwire-Mangen, F. and Meehan, M. O. and Lutalo, T. and Gray, R. H.},
  Journal                  = {New England Journal of Medicine},
