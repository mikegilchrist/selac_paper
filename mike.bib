@Article{HideoMatsumura12232003,
  Title                    = {{Gene expression analysis of plant host-pathogen interactions by SuperSAGE}},
  Author                   = {Matsumura, Hideo and Reich, Stefanie and Ito, Akiko and Saitoh, Hiromasa and Kamoun, Sophien and Winter, Peter and Kahl, Gunter and Reuter, Monika and Kruger, Detlev H. and Terauchi, Ryohei},
