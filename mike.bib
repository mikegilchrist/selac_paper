  Keywords                 = {SACCHAROMYCES-CEREVISIAE; TRANSLATIONAL SELECTION; DROSOPHILA-MELANOGASTER; UNICELLULAR ORGANISMS; ENTAMOEBA-HISTOLYTICA; ESCHERICHIA-COLI; EXPRESSED GENES; G+C CONTENT; GENOME; BIAS}
}

@Article{GromadskiAndRodnina2004,
  Title                    = {Kinetic determinants of high-fidelity tRNA discrimination on the ribosome},
