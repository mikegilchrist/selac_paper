  Title                    = {How to calculate the non-synonymous to synonymous rate ratio of protein-coding genes under the Fisher-Wright mutation-selection framework},
  Author                   = {dos Reis, M.},
  Journal                  = {Biology Letters},
  Year                     = {2015},
