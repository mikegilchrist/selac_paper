@Article{KermackAndMcKendrick1932,
  Title                    = {Contributions to the mathematical theory of epidemics. {II}.- the problem of endemicity},
  Author                   = {Kermack, W. O and McKendrick A.G.},
  Journal                  = {Proceedings of the Royal Society of London A, Containing Papers of a Mathematical and Physical Character},
  Year                     = {1932},
  Pages                    = {55-83},
  Volume                   = {138},

  Annote                   = { Intro: "It is the purpose of the present paper to consider the effect of the continuous introduction of fresh susceptible individuals intot he population. Mention some experiments by "Topley and others -- where fresh animals were introduced at a constant rate into the cages in which cases of disease were already present\ldots" "an attempt has also been made to include partial immunity which may follow an attack." Again they start with the case of constant parameters, but then do an analysis using arbitrary general functions. Additional analyses of five special cases are presented in part III. Assume only one disease and that its the only source of mortality. Model: Constant-- Define model, do a stability analysis about equilibrium and show that they are generally stable, potentially oscillatory. They then explore five special cases (no imm. , no births, disease fatal, etc). Results are summarized in Table I Variable rates: allow recovery, added mort, etc to vary with age of an infection. Results are summarized in Table II. Discussion:In general the find that there can be at most one steady state. If these open populations exceed the threshold, then a certain proportion will become sick, but that number will not be as large as the total amount the population exceeds the threshold (why? I don't know) although the disease rate (i.e. the rate of disease spread?) will be proportional to the excess when the excess is small. Equilibriums can be stable or unstable, but are generally stable in the cases with constant rates.}
}

@Article{KermackAndMcKendrick1927,
  Title                    = {A contribution to the mathematical theory of epidemics},
  Author                   = {Kermack, W. O. and McKendrick, A. G.},
  Journal                  = {Proceedings of the Royal Society of London A, Containing Papers of a Mathematical and Physical Character},
  Year                     = {1927},
  Pages                    = {700-721},
  Volume                   = {115},

  Annote                   = {They reference some earlier work by Ross and Hudson in 1915-1917, but apparently this is a generalization of their approach. The problem they are addressing is "One (or more) infected person ins introd. into a community of ind. more or less suc. to the disease in question. The disease spreads from the affect to the unaffected by contact infection. Each infected person runs thorugh the course of his sickness, and finally is removed from the number of those who are sick, by recovery or death\ldots One of the most import. problems in epidemiology is to ascertain whether this termination occurs only when no susecptible ind. are left or \ldots whilst many suscept. ind. are still present in the unaffected pop." Define threshold density for disease spread w/in a pop. "It will appear also that the size of the epidemic increases rapidly as the threshold density is eceeded, and in such a manner that the greater the pop. density at the beginning of the epidemic, the smaller it will be at the end of the epidemic." This is b/c the epidemic will continue to grow so long as the density of suscept. is above the threshold. Interestingly they say that two reasons often given for the "termination of an epidemic" is the loss of ALL susceptibles or "the virulence of the causative organism has gradually decreased." They then say neither view is supported. Models: Start with a discrete time model where host population is structured by the age of the infection. Cool. For simplicity and b/c they are not interested in a growing pop. they lump dead and recovered together. They then take the limit of their time unit going to 0 and get the continous time version with survivorship of the original infected cohort as a function of $\exp[\int_0^t \psi(a) da]$. In general this is a very humbling paper. Lots of math and Laplace transforms, etc. Specific Case: Constant characteristic rates. Getting to the biology they note that if the population is initially equal to $N_T + n$ then the total number of infected hosts that become infected is equal to $2 n - 2 n^2/N $ such that in the end if $n\ll N$, then, $N(\infty) \approx N_T - n$. General Case: Varying characteristic rates. Essentially the same results in terms of $N(\infty)$ They also discuss how a change in infectivity (i.e. transmission coefficient) can lead to an epidemic as well if the host population is at the threshold density. They then make a quick step to indirectly transmitted diseases (plague, malaria) and argue that if $N_v N_h > N_T_v N_T_h$ then an epidemic will occur. They end by discussing how "in many regions of the world the acutall density of a population may not be widely different from the threshold density with regard to some dominant contagious disease." 3/01}
}

@Article{KhaleghpourEtAl2001,
  Title                    = {Translational repression by a novel partner of human poly{(A)} binding protein, {PAIP2}},
  Author                   = {Khaleghpour, K. and Svitkin, Y. V. and Craig, A. W. and DeMaria, C. T. and Deo, R. C. and Burley, S. K. and Sonenberg, N.},
  Journal                  = {Molecular Cell},
  Year                     = {2001},
  Pages                    = {205-216},
  Volume                   = {7},

  Abstract                 = {The eukaryotic {{mRNA}} 3' poly({A}) tail acts synergistically with the 5' cap structure to enhance translation. This effect is mediated by a bridging complex, composed of the poly(A) binding protein (PABP), eIF4G, and the cap binding protein, eIF4E. PABP-interacting protein 1 (Paip1) is another factor that interacts with PABP to coactivate translation. Here, we describe a novel human PABP-interacting protein (Paip2), which acts as a repressor of translation both \emph{in vitro} and \emph{in vivo}. Paip2 preferentially inhibits translation of a poly(A)-containing {mRNA}, but has no effect on the translation of hepatitis C virus {mRNA}, which is capand eIf4G-independent. Paip2 decreases the affinity of PABP for polyadenylate RNA, and disrupts the repeating structure of poly(A) ribonucleoprotein. Furthermore, Paip2 competes with Paip1 for PABP binding. Thus, Paip2 inhibits translation by interdicting PABP function. }
}

