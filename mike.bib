@Article{KauppiEtAl2009,
  Title                    = {Analysis of meiotic recombination products from human sperm.},
  Author                   = {Kauppi, L. and May, C. A. and Jeffreys, A. J.},
  Journal                  = {Methods in molecular biology (Clifton, N.J.)},
  Year                     = {2009},
  Pages                    = {323-55},
  Volume                   = {557},

  Abstract                 = {Traditional methods for surveying meiotic recombination in humans are limited to pedigree and linkage disequilibrium analyses. We have developed assays that allow the direct detection of crossover and gene conversion molecules in batches of sperm DNA. To date, we have characterized 26 recombination hotspots by allele-specific PCR and selectively amplified recombinant DNA molecules from these regions. These analyses have revealed that meiotic crossover hotspots in humans are highly localized and flanked by DNA segments where recombination is suppressed. The centers of crossover hotspots are also active in noncrossover recombination, displaying short conversion tracts.},
  File                     = {Kauppi.et.al.09.pdf:/home/mikeg/References/Kauppi.et.al.09.pdf:PDF},
