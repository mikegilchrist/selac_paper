  Journal-iso              = {Nature},
  Keywords-plus            = {GENOME-WIDE ASSOCIATION; EMBRYONIC STEM-CELLS; DYNAMIC DNA METHYLATION; CHROMATIN-STATE; HISTONE MODIFICATIONS; RISK LOCI; TRANSCRIPTION FACTORS; ALZHEIMERS-DISEASE; REGULATORY REGIONS; MAMMALIAN GENOMES},
