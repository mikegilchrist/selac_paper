@Article{AntonovicsAndThrall1994,
  Title                    = {The cost of resistance and the maintenance of genetic polymorphism in host-pathogen systems},
  Author                   = {Antonovics, Janis and Thrall, Peter, H.},
  Journal                  = {Proceedings of the Royal Society of London B, Biological Sciences},
  Year                     = {1994},
  Pages                    = {105-110},
  Volume                   = {257},

  Annote                   = { Intro: Mentions Simms (1992a) as a good intro to biological basis of the cost of resistance. Gillespie (1975) as first to incorp. genetics and epidemiology making the evolution of the system both a freq and density dep process. In the current study they compare two transmission modes: freq and dens. dependent. Model: Cost for resistance in terms of reduced birth rate. Resistance is a continuous trait. Continuous time model. Background mort. No recovery from infection. Density dependent birth rate (logisitic) Force of the infection is either prop. to freq. or density of infect. hosts. Find criteria for persistance of polymorphism at equilibrium, do stability analysis. They note that if one ignored host density effects such that the size of the population stayed constant then "It is straightforward to show that no polymorphic equilibrium exists under these conditions". Apparently this is similar to what Jayakar (1970) found. Discussion: "the expectation that hihgly resistant ind. should incur a correspondingly high cost to coexist with susceptibles is not borne out." This is because there is density dependent host growth and, therefore, the resistant hosts reduce the force of infection by diluting the suscept. population. Region of polymorphism becomes more restricted as the two genotypes become more similar in their suscept. "This is because the intro. of an alternative allele that differs only slightly in resistance has little effect on the disease incidence." It follows that one would expect to find extreme, rather than slight, differences in variation in natural populations which is apparently the case. Cite Kemper (1982) "It is interesting that the equilibrium numbers of diseased individual in the density-dependent transmission case is simply the cost-benefit ratio and independent of $\gamma$ (the degree of density-dependent population regulation)." They note that this finding is similar to that of Gillespie (1975). }
}

@Article{AparicioEtAl2004,
  Title                    = {Competition and coexistence in host-parasite systems: the myxomatosis case},
  Author                   = {Aparicio, J. P. and Solari, H. G. and Bonino, N.},
  Journal                  = {Population Ecology},
  Year                     = {2004},
  Pages                    = {71-85},
  Volume                   = {46},

  Abstract                 = {Co-circulation of several strains of parasites has been observed in many host-parasite systems. However, simple epidemiological models cannot sustain this coexistence. In this work we study the coexistence of viral strains in the myxomatosis case. Myxomatosis, a highly lethal disease of the European rabbit, has been used in Australia and Europe as a biological control of rabbit populations. A few years after its introduction, the original highly virulent strains were almost completely replaced by field strains covering a wide range of virulence. Here, we study several mechanisms that may explain the field observations. First we considered spatial heterogeneity. The establishment of any strain over regions occupied by host populations may delay the spread of any superior competitive virus strain, producing global coexistence in the long term. On the other hand, sub-populations with different resistance levels in epidemiological contact, as observed in the field, can maintain several different virus strains co-circulating. The second class of mechanism introduces diversity among hosts of a local population sharing a territory. We considered different classes of host resistance to myxomatosis: belonging to a resistance class is a random fact. Host age-dependent resistance is also especially considered. These types of population heterogeneity can sustain local coexistence for many years, although exclusion takes place for long enough periods. The concurrent action of both types of mechanisms could explain why the diversity of virus strains is sustained, and the local coexistence. Finally, we briefly discuss the influence of host genetic dynamics in the coevolution of the system. }
}

