@Article{KoshiEtAl1999,
  Title                    = {Using physical-chemistry-based substitution models in phylogenetic analyses of HIV-1 subtypes.},
  Author                   = {Koshi, J. M. and Mindell, D. P. and Goldstein, R. A.},
  Journal                  = {Molecular biology and evolution},
  Year                     = {1999},
  Pages                    = {173-179},
  Volume                   = {16},

  Abstract                 = {HIV-1 subtype phylogeny is investigated using a previously developed computational model of natural amino acid site substitutions. This model, based on Boltzmann statistics and Metropolis kinetics, involves an order of magnitude fewer adjustable parameters than traditional substitution matrices and deals more effectively with the issue of protein site heterogeneity. When optimized for sequences of HIV-1 envelope (env) proteins from a few specific subtypes, our model is more likely to describe the evolutionary record for other subtypes than are methods using a single substitution matrix, even a matrix optimized over the same data. Pairwise distances are calculated between various probabilistic ancestral subtype sequences, and a distance matrix approach is used to find the optimal phylogenetic tree. Our results indicate that the relationships between subtypes B, C, and D and those between subtypes A and H may be closer than previously thought. ER},
  File                     = {Koshi.et.al_1999_using.physical-chemistry-based.substitution.models.in.phylogenetic.analyses.of.HIV-1.subtypes_Molecular.biology.and.evolution.pdf:/home/mikeg/References/Koshi.et.al_1999_using.physical-chemistry-based.substitution.models.in.phylogenetic.analyses.of.hIV-1.subtypes_Molecular.biology.and.evolution.pdf:PDF},
  Journal-iso              = {Mol. Biol. Evol.},
