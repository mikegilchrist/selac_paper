  Owner                    = {mikeg},
  Pii                      = {S0959-440X(09)00124-9},
  Pmid                     = {19765975},
  Timestamp                = {2009.12.18},
  Url                      = {http://dx.doi.org/10.1016/j.sbi.2009.08.003}
}

@Article{TonellaEtAl2001,
  Title                    = {New perspectives in the \emph{{E}scherichia coli} proteome investigation},
