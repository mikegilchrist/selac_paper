}

@Article{NorambuenaEtAl2007,
  Title                    = {SAGExplore: a web server for unambiguous tag mapping in serial analysis of gene expression oriented to gene discovery and annotation},
  Author                   = {Norambuena, T. and Malig, R. and Melo, F.},
