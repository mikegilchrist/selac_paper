@Article{MannaEtAl2009,
  Title                    = {Evolutionary constraints on hub and non-hub proteins in human protein interaction network: Insight from protein connectivity and intrinsic disorder},
  Author                   = {Manna, B. and Bhattacharya, T. and Kahali, B. and Ghosh, T. C.},
