@Article{SolomoviciEtAl1997,
  Title                    = {Does \emph{{E}scherichia coli} optimize the economics of the translation process?},
  Author                   = {Solomovici, J. and Lesnik, T. and Reiss, C.},
  Journal                  = {Journal of Theoretical Biology},
  Year                     = {1997},
  Pages                    = {511-521},
  Volume                   = {185},

  Abstract                 = {The codon translation rate is usually assumed to be proportional to the cellular concentration of the cognate {tRNA}, but synonymous codons sharing the same cognate {tRNA} may be translated at rather different rates. To account for the latter observation, we assume that the translation process is optimized in two respects: (i), the codon demand is optimized with respect to the supply of cognate {tRNA}s (composition of the {tRNA} pool); and (ii), for synonymous codons sharing the same cognate {tRNA}, the usage frequency of each codon correlates optimally with the stability of the codon-anticodon complex. These assumptions allow us to compute the relative rate constants of synonymous codons. Highly expressed genes, which produce 80-90\% of the protein mass in the E. coli cell, appear to have selected codons which make an optimal use of the {tRNA} pool. Assuming the optimization criteria were valid, a list of codon translation times (in ms) were derived from available experimental data. (C) 1997 Academic Press Limited.},
  File                     = {Solomovici.et.al.97.pdf:/home/mikeg/References/Solomovici.et.al.97.pdf:PDF}
}

