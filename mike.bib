@Article{Plutynski2008,
  Title                    = {Explaining how and explaining why: developmental and evolutionary explanations of dominance},
  Author                   = {Anya Plutynski},
  Journal                  = {Biology and Philosophy},
  Year                     = {2008},

  Month                    = jun,
  Pages                    = {363-381},
  Volume                   = {23},

  Abstract                 = {Abstract There have been two different schools of thought on the evolution of dominance. On the one hand, followers of Wright [Wright S. 1929. Am. Nat. 63: 274{\textendash}279, Evolution: Selected Papers by Sewall Wright, University of Chicago Press, Chicago; 1934. Am. Nat. 68: 25{\textendash}53, Evolution: Selected Papers by Sewall Wright, University of Chicago Press, Chicago; Haldane J.B.S. 1930. Am. Nat. 64: 87{\textendash}90; 1939. J. Genet. 37: 365{\textendash}374; Kacser H. and Burns J.A. 1981. Genetics 97: 639{\textendash}666] have defended the view that dominance is a product of non-linearities in gene expression. On the other hand, followers of Fisher [Fisher R.A. 1928a. Am. Nat. 62: 15{\textendash}126; 1928b. Am. Nat. 62: 571{\textendash}574; B\~{u}rger R. 1983a. Math. Biosci. 67: 125{\textendash}143; 1983b. J. Math. Biol. 16: 269{\textendash}280; Wagner G. and Burger R. 1985. J. Theor. Biol. 113: 475{\textendash}500; Mayo O. and Reinhard B. 1997. Biol. Rev. 72: 97{\textendash}110] have argued that dominance evolved via selection on modifier genes. Some have called these {\textquotedblleft}physiological{\textquotedblright} versus {\textquotedblleft}selectionist,{\textquotedblright} or more recently [Falk R. 2001. Biol. Philos. 16: 285{\textendash}323], {\textquotedblleft}functional,{\textquotedblright} versus {\textquotedblleft}structural{\textquotedblright} explanations of dominance. This paper argues, however, that one need not treat these explanations as exclusive. While one can disagree about the most likely evolutionary explanation of dominance, as Wright and Fisher did, offering a {\textquotedblleft}physiological{\textquotedblright} or developmental explanation of dominance does not render dominance {\textquotedblleft}epiphenomenal,{\textquotedblright} nor show that evolutionary considerations are irrelevant to the maintenance of dominance, as some [Kacser H. and Burns J.A. 1981. Genetics 97: 639{\textendash}666] have argued. Recent work [Gilchrist M.A. and Nijhout H.F. 2001. Genetics 159: 423{\textendash}432] illustrates how biological explanation is a multi-level task, requiring both a {\textquotedblleft}top-down{\textquotedblright} approach to understanding how a pattern of inheritance or trait might be maintained in populations, as well as {\textquotedblleft}bottom-up{\textquotedblright} modeling of the dynamics of gene expression. },
  Doi                      = {10.1007/s10539-006-9047-5},
  Url                      = {http://dx.doi.org/10.1007/s10539-006-9047-5}
}

