@Article{GomoulkiewiczEtAl2000,
  Title                    = {Hot spots, cold spots and the geographic mosaic theory of coevolution},
  Author                   = {Goulkiewicz, Richard and Thompson, John N. and Holt, Robert D. and Nuismer, Scott L. and Hochberg, Michael E.},
  Journal                  = {The American Naturalist},
  Year                     = {2000},
  Pages                    = {156-174},
  Volume                   = {156},

  Annote                   = { Stats with a list of empirical systems in which there "pairs of species\ldots composed of collections of geographically differentiated populations." Examples include flax and flax rust, snails and trematodes, Drosp. and its parasitoid, legumes and rhizobia, crossbilss and lodgepole pines, garter snakes and Tarica, wild parsnip and webworms, yucca and yucca moths, Greya moths and host plants. "All these interaction show geographic diff. in gen. struct. of the interact. pop. and in the traits import. to the assoc." Apparently, "the geographic mosaic theory of coevolution argues that the overall coevol. dynamics of such interactors are driven by .. selection mosaics, coevol. host spots and trait remixing." Not sure what the hell that means. Selection mosaics: when ns on interactions varies among diff. comm. Hotspots: comm. in which interacting spp. have reciprocal effects on fitness and are embedded in areas in which there is selection on only one or none of the species (cold spots). I'm not clear what all the fuss is. The basic model assumes that allele fitness of species $x$ is a function of allele freq of species $y$ in both hot and cold spots. Species $y$ fitness, however, is only of function of species $x$ in hot spots (it's constant otherwise). Spatially implicit in only the most rudimentory sense. There are essentially two patches. Explore different amounts of gene flow and under both soft selection (local regulation of density within a patch, independent of selection) and hard selection (proportion of adults emerging from a spot is a function of selection and total population regulation occurs globally). Assume positive freq. dependent selection, i.e. fitness of an allele increases or decreases linearly with frequency of the allele of the other species. Isolated spots: no gene flow from h to c or c to h. Five possible equilibria. Four corner and one internal which is always unstable. The stability of the corners depends on the sign of the interaction. Hot spots: If the interaction is antagonistic then "mismatched alleles" are stable. If the interaction is cooperative then matche alleles become fixed. Victim-exploiter is unstable, but eventually becomes fixed arbitrarily for finite systems. Uniform mixing: Variation in habitats, but gene frequency across entire system is looked at since there is uniform mixing (i.e. prob. of offspring going to hot or cold spot is independent of origin and is simply proportional to fraction of habitat which is hot or cold). Hard Selection: okay. there's really nothing that profound in all of this. There are, however, some cool graphs (check out Fig 7).}
}

@Article{GourbiereEtAl1999,
  Title                    = {Proportion of needles colonized by one fungal species in coniferous litter: the dispersal hypothesis},
  Author                   = {Gourbiere, F. and Gourbiere, S. and Van Maanen, A. and Vallet, G. and Auger, P.},
  Journal                  = {Mycological Research},
  Year                     = {1999},
  Pages                    = {353-359},
  Volume                   = {103},

  Abstract                 = {We try to explain the large differences observed in the proportion of needles colonized by fungal species in coniferous litter by spore dispersal properties. We constructed a population dynamics model, assuming that litter is a resource that is fragmented into ephemeral units, periodically produced. Spore dispersion on needles on a small spatial scale was assumed to follow a Poisson distribution. The model was compared with an experimental system using Thysanophora penicillioides on Abies alba needles and its relevance to field conditions is discussed.}
}

@Article{GourbiereEtAl2001,
  Title                    = {Associations between three fungi on pine needles and their variation along a climatic gradient},
  Author                   = {Gourbiere, F. and van Maanen, A. and Debouzie, D.},
  Journal                  = {Mycological Research},
  Year                     = {2001},
  Pages                    = {1101-1109},
  Volume                   = {105},

  Abstract                 = {This paper develops field data analysis and theoretical hypotheses concerning the role of climatic factors and fungal interactions in the control of fungal populations. We studied three 'resource unit restricted fungi' colonizing Pinus sylvestris needles along a climatic gradient. We investigated both competition and succession. A general linear model was used for statistical analysis. Despite interference competition at the scale of the individual needle, the first two colonizers, Lophodermium pinastri and Cyclaneusma minus, coexisted and the frequencies of both increased with altitude. This increase seemed to be related to the greater humidity at altitude. The initial stages of development of the final colonizer, Verticicladium. trifidum, were strongly dependent on the first two colonizers on the individual needles. The observed decrease in frequency of V. trifidum with increasing altitude may be due to interactions with the early colonizers, with no direct effect of altitude on V. trifidum. Fungal colonization at low and high altitudes can be interpreted by accounting for both climatic effects and fungal interactions expressed as interference and succession. }
}

@Article{GourbiereAndGourbiere2002,
  Title                    = {Competition between unit-restricted fungi: A metapopulation model},
  Author                   = {Gourbiere, S. and Gourbiere, F.},
  Journal                  = {Journal of Theoretical Biology},
  Year                     = {2002},
  Pages                    = {351-368},
  Volume                   = {217},

  Abstract                 = {We aimed to provide a theoretical framework for dynamic studies of competition between fungi living on divided and ephemeral resources. We previously adapted the seminal Skellam's patch-occupancy model (Skellam, 1951) to describe the population dynamics of one species of unit-restricted fungus whose mycelial growth occurs within resource units and which colonizes new resource units by spore dispersal (Gourbiere et al., 1999). In this study, we extended this model to describe the competition between a pair of unit-restricted fungal species that interact. with each other inside units by decreasing their spore production. Accordingly, we designed a discrete-time metapopulation model where all patches go extinct at each generation and species interact by lowering their propagule production in jointly occupied patches. We showed that the two species easily coexist although there is no trade-off between their competitive and colonization abilities. Furthermore, the outcome of the competition process can depend on a founder effect. Founder effect determines either which species is excluded or the relative densities of each species when they coexist. We investigated the implications of these results on the distribution and abundance of fungal species along environmental gradients. This work bridges the gap between the mycological theory of "Resource Units" and the metapopulation theory, showing the specificity of fungal exploitation competition. We suggest that unit-restricted fungal species are appropriate biological models to test the theoretical results of the metapopulation theory, such as the appearance of alternative stable equilibria. (C) 2002 Elsevier Science Ltd. All rights reserved.}
}

