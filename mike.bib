@Article{CortazzoEtAl2002,
  Title                    = {Silent mutations affect \emph{in vivo} protein folding in \emph{{E}scherichia coli}},
  Author                   = {Cortazzo, P. and Cervenansky, C. and Marin, M. and Reiss, C. and Ehrlich, R. and Deana, A.},
  Journal                  = {Biochemical and Biophysical Research Communications},
  Year                     = {2002},
  Pages                    = {537--541},
  Volume                   = {293},

  Abstract                 = {As an approach to investigate the molecular mechanism of \emph{in vivo} protein folding and the role of translation kinetics on specific folding pathways, we made codon substitutions in the EgFABP1 (Echinococcus granulosus fatty acid binding protein1) gene that replaced five minor codons with their synonymous major ones. The altered region corresponds to a turn between two short alpha helices. One of the silent mutations of EgFABP1 markedly decreased the solubility of the protein when expressed in \emph{{E}scherichia coli}. Expression of this protein also caused strong activation of a reporter gene designed to detect misfolded proteins, suggesting that the turn region seems to have special translation kinetic requirements that ensure proper folding of the protein. Our results highlight the importance of codon usage in the \emph{in vivo} protein folding. (C) 2002 Elsevier Science (USA). All rights reserved.},
  Optmonth                 = {April},
