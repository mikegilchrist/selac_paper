@Article{WagnerAndMezey,
  Title                    = {Modeling the evolution of genetic architecture: A continuum of alleles model with pairwise A x A epistasis},
  Author                   = {Wagner, G{}nther P. and Mezey, Jason},
  Journal                  = {Journal of Theoretical Biology},

  Abstract                 = {"A continuum of alleles model with pair-wise A x A epistasis is proposed and its transmission genetic, and variational properties are analysed. The basic idea is that genes control the values of underlying variables, which affect the genotypic value of phenotypic characters proportional to a "scaling factor". Epistasis is the influence of one gene on the average effect of another gene. In this model, epistasis is introduced as a mutational effect of one gene on the scaling factors of another gene. In accordance with empirical results, the model assumes that the average direct effect of mutations is zero, as is the average epistatic effect. The model predicts that, on average, a mutation at one locus increases the expected mutational variance of mutations at another interacting locus. The increase in mutational variance is predicted to be equal to the variance of the pair-wise epistatic effects. This result is consistent with the observation that mutant phenotypes tend to be more variable than the wildtype phenotype. Another generic result of this model is that the frequency of canalizing mutations can at most be equal to the frequency of decanalizing mutations. Furthermore, it is predicted that the mutational variance of a character increases at least linearly with the size of the character; hence this model is scale variant. In the case of two characters it is shown that the dimensionality of the locus-specific mutational effect distribution is invariant, i.e. the rank of the mutational covariance matrix M is invariant. While in additive models the mutational covariance matrix is always and entirely invariant, the invariance in the case of epistatic models is unexpected. Epistatic interactions can change the magnitude of the mutational (co)variances at a locus and can thus influence the structure of the mutational covariance matrix. However, in the present model the dimensionality of the mutational effect distribution remains the same. A consequence of this result is that, in this model, the genetic architecture of a set of characters is always evolvable i.e. no hard constraints can evolve"}
}

@InCollection{Wakelin1994,
  Title                    = {Host populations: genetics and immunity},
  Author                   = {Wakelin, D.},
  Booktitle                = {Parasitic and Infectious diseases: Epidemiology and Ecology},
  Publisher                = {Academic Press},
  Year                     = {1994},

  Address                  = {San Diego},
  Editor                   = {Scott, Marilyn E. and Smith, Gary},
  Pages                    = {83-100},

  Annote                   = {A reasonably shor discussion of the sources of potential genetic variation b/w mammalian hosts which leads to variation in resistance and susceptibility. Apparently immunogeneticists classify genes in two ways, MHC related and background genes (i.e. non-MHC). Many components to mammalian immune response against parasites, "Many oof these response may have no, or only marginal, activity against the targe parasite; some may be detrimental to the host; brobably only a few constitute the majore effectors of resistance." So it seems like there are lots of weapons in general, but in each case only a few are effective against any given parasite. Variation in the ability of these effective ones b/w hosts equals variation in host resistance. MHC related genes appear to be those that determine which epitopes are presented and how and the recognition genes on the T-cells for the epitopes but barring major mutational problems, "variations in resistance, suscept. and immunopathology more often reflect the activities of background genes\ldots[which] regulate all immunological and inflammatory cell pop., determining their devel. and freq. and controlling their productionof receptors and mediators." Major points about genes and Ir \begin{enumerate} \item of the many epitopes and antigens present by parasites, on a few are important to the h-p relationship \item of the man immune and inflamm. responses, on certain responses exert a sig. influence on parasite. \item Var. in responses exist in most pop. \end{enumerate} Part III genetic influcence on Immunity to infection A. Protozoa and other microparasites. Replicate within the host. "Therefore the llevels of infection within individulals are determined primarily by the capacity of hte host to control this replication." 1. Malaria: First level of immunity related to polymorphs related to target red-cells that affect parasite's ability to invade or develop. MHC appears to have little influence b/c in general there is substantial var. b/w parasites and lots of potential epitopes. Antibody titer and Tcell mediated responses may be important 2. Leishmania: Infects macrophages, apparently. Lots of variation in responses to parasite which lead to variation in disease responses. MHC and background genes are involved. Susceptibility to visceral leishmaniasis has been linked to a single gene Lsh (in mouse?) and appears to also control suscept. to natural mouse parasitic spp. of Samonella and Mycobacterium. Lsh is related to macrophage's suitability for intracellular pathogens. Other genes have also been identified for controlling infection of other Leishmania spp. 3. Intestinal Protozoa: "Among the most common of all infections\ldots" Genetic variation in mice has been detected for suscp. to a number of different spp. including Giardia and Eimeria. Variation for Giardia related to ability to fully clear infection or if it lingers. May be related to IgA deficiency. B. Helminths: larger than most protozoan parasite targets, therefore, require other aspects of IR to clear. Also have protective surface layer and may be located in different parts of organism which "\ldots may circumscribe the mech. that are otherewise able to exert deleterious effects" of the IR on the parasite. 1. Schistosomes: Lots of variation in load b/w hosts w/ equal levels of exposure. IgE responses seem to be important in reisstnace. Apparently this may be the result of a "well est." trade-off b/w IgE and IgG$_4$ production which is determined by " the balance of active T cell subsets and their assoc. cytokine profiles. One possibility is, therefore, that suscp. individuals are progreammed to prodeuce the 'wrong' antibody responses can, in fact, block protective immune mech. and maintain chronic infection in the host." 2. Tapeworms: Variation in mice to est. of larval Taenia teniaeformis is " now known to be expressed through background genes that regulate the speed at whidch antiparasite antibody is produced. In rapid responders complement-fixing antibodies become avalilable before the larval worms develop an effective anticomplementary activity." Talks about a study of a natural pop. of deer mice and adult tapeworms. Distn. across hosts was highly aggregated and apparently due to a single gene which was due to a single dominant resist allele. Mech behind resistance is unknown. Research group headed by Wassom (70s and 80s) 3. Gastrointestinal nematodes: "among the most common [infections] experience by humans and [domesticated?] animal hosts." Agg. infect. in hosts could be due to gen. var. observed in lab. Several spp. "reach intestine afeter pereneral migrations (e.g. Ascaris, hookworms, \ldots) antiworm [antibody] responses can act against theses migrating stages, particularly during reinfections." Lots of different aspects of IR can become involved. Ab "may play an import. role against a number of gastro. nematodes. The binidng o fthe antibody to target molec. present ont the cuticle and intest. epithelium... can interfere with a variety of essent. activites. includ. behavior., mutrit. reprod. and defense mech." Impt. ab include: IgG, IgA, and IgE in fact, GI "nematodes elicit strong IgE responses, and this isotype has long been implicated in protect. immunity through its involvement in immediate hypersensitivity reactions\ldots" Both MHC and background genes are important. 4. Filarial nematodes: Apperently family of nematodes that are transmitted via arthropod vectors. Lots of var. in humans to suscept. "Infective larvae seem to be import. targests of IR leading to resist., whereas responses directed against adult and microfilarial stages may lead to pathology." C. Arthropods: V. Conclusion: "The realization that relatively few genes exert bilogocially significant effects on host resistance makes it possible to see that evolutionary change in the balance of a relationship can occur as a result of small alterations in parasite or host activity." It seems that for each case of resistance or suscept. only a few genes are involved. These are the key potential weak links in the system and my guess is that which links are the weak ones may shift over time as the host-evolves and counter evolves or, if there is constrain on the host, would be static reaching a stable polymorphism. Now there's a good question. If the key genes shift then it would appear that the red queen hypoth. could apply for maint. of sex and other things, but if they don't, then it doesn't apply. Are there empirical systems in which this could be tested? Bacterial-phage would be ideal, in some sense, but they may lack the required complexity? It seems like to get links to shift noticably you need a long chain (i.e. a complex IR response), but if this were true, then that means r.q. would apply more to org. with complex IR (which are assumedly longer living org.). Imagine two scenarios: one where the suscept/resist of the h-p interaction is always centered at one place (e.g. initial Ab recog) such as influenza virus variation b/w years vs. a system where the determinant of s/r changes over time. One would be driven by var. in parasite, perhaps, the other, var. in host that the parasite then evolves to exploit. . }
}

@Article{WaldmanEtAl2011,
  Title                    = {Selection for Translation Efficiency on Synonymous Polymorphisms in Recent Human Evolution},
  Author                   = {Waldman, Y. Y. and Tuller, T. and Keinan, A. and Ruppin, E.},
  Journal                  = {Genome Biology And Evolution},
  Year                     = {2011},
  Pages                    = {749-761},
  Volume                   = {3},

  Abstract                 = {Synonymous mutations are considered to be "silent" as they do not affect protein sequence. However, different silent codons have different translation efficiency (TE), which raises the question to what extent such mutations are really neutral. We perform the first genome-wide study of natural selection operating on TE in recent human evolution, surveying 13,798 synonymous single nucleotide polymorphisms (SNPs) in 1,198 unrelated individuals from 11 populations. We find evidence for both negative and positive selection on TE, as measured based on differentiation in allele frequencies between populations. Notably, the likelihood of an SNP to be targeted by positive or negative selection is correlated with the magnitude of its effect on the TE of the corresponding protein. Furthermore, negative selection acting against changes in TE is more marked in highly expressed genes, highly interacting proteins, complex members, and regulatory genes. It is also more common in functional regions and in the initial segments of highly expressed genes. Positive selection targeting sites with a large effect on TE is stronger in lowly interacting proteins and in regulatory genes. Similarly, essential genes are enriched for negative TE selection while underrepresented for positive TE selection. Taken together, these results point to the significant role of TE as a selective force operating in humans and hence underscore the importance of considering silent SNPs in interpreting associations with complex human diseases. Testifying to this potential, we describe two synonymous SNPs that may have clinical implications in phenylketonuria and in Best's macular dystrophy due to TE differences between alleles.},
  File                     = {Waldman.et.al.11.pdf:/home/mikeg/References/Waldman.et.al.11.pdf:PDF},
  Journal-iso              = {Genome Biol. Evol.},
  Keywords                 = {PROTEIN-INTERACTION NETWORK; TRANSFER-RNA GENES; CODON USAGE; POSITIVE SELECTION; NATURAL-SELECTION; HUMAN GENOME; HUMAN-POPULATIONS; ESCHERICHIA-COLI; TISSUE-SPECIFICITY; RESPECTIVE CODONS},
  Timestamp                = {2015.3.16}
}

@Article{Walker2007,
  Title                    = {Huntington's disease},
  Author                   = {Francis O Walker},
  Journal                  = {The Lancet},
  Year                     = {2007},
  Number                   = {9557},
  Pages                    = {218 - 228},
  Volume                   = {369},

  Abstract                 = {Summary Huntington's disease is an autosomal-dominant, progressive neurodegenerative disorder with a distinct phenotype, including chorea and dystonia, incoordination, cognitive decline, and behavioural difficulties. Typically, onset of symptoms is in middle-age after affected individuals have had children, but the disorder can manifest at any time between infancy and senescence. The mutant protein in Huntington's disease--huntingtin--results from an expanded CAG repeat leading to a polyglutamine strand of variable length at the N-terminus. Evidence suggests that this tail confers a toxic gain of function. The precise pathophysiological mechanisms of Huntington's disease are poorly understood, but research in transgenic animal models of the disorder is providing insight into causative factors and potential treatments.},
