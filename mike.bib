@Article{Ewald2009,
  Title                    = {An Evolutionary Perspective on Parasitism as a Cause of Cancer},
  Author                   = {Ewald, P. W.},
  Journal                  = {Advances In Parasitology, Vol 68: Natural History Of Host-Parasite Interactions},
  Year                     = {2009},
