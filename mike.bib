@Article{KonevegaEtAl2006,
  Title                    = {The effect of modification of t{RNA} nucleotide-37 on the t{RNA} interaction with the P- and A-site of the 70s ribosome \emph{{E}scherichia coli} RID B-7887-2008},
  Author                   = {Konevega, A. L. and Soboleva, N. G. and Makhno, V. I. and Peshehonov, A. V. and Katunin, V. I.},
