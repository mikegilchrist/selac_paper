  Keywords                 = {Amino Acid Substitution, genetics/immunology; Artifacts; Base Composition; Base Pair Mismatch; Cell Line; Chromosomes, Human, Pair 6, genetics; Codon, genetics; DNA Primers, chemical synthesis; Ethnic Groups, genetics; HLA Antigens, genetics; Humans; Major Histocompatibility Complex, genetics; Membrane Proteins; Polymerase Chain Reaction, methods; Polymorphism, Genetic; Receptors, Cell Surface, genetics; Receptors, Odorant, genetics; South Dakota},
  Language                 = {eng},
  Medline-pst              = {ppublish},
