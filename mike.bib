@Article{MayAndAnderson1978,
  Title                    = {Regulation and stability of host-parsasite population interactions: {II}. {D}estabilizaing processes},
  Author                   = {May, Robert M. and Anderson, Roy M.},
  Journal                  = {Journal of Animal Ecology},
  Year                     = {1978},
  Pages                    = {249-267},
  Volume                   = {47},

  Annote                   = {explore relationships which tend to destabilize the h-p system. Model D: parasite induced reduction in host repro. Model E: Within-host parsite replication Model F: Time delays in parasite repro. and transmission Also "\ldots give a general discussion (buttressed with empirical observations) of the regulatory influence of aprasites in natural populations of animals." Model D: Sterilizing effects: e.g. of \textit{Schistosoma haematobium} on its molluscan host \textit{Biomphalaria truncatus}. Parasitic castration has been observed by protozoa, helminths, and arthropods parasites. Assume that host repro rate decreases linearly with p load. Thus the equation describing parasite population growth doesn't change, only that for the host. Derive criteria for existance of an equilibrium. If equilibrium exists, stability criteria is $\alpha > \beta k$ where $\alpha$ is the added host mort/parasite, $\beta$ is the decrease in host reprod. rate/parasite and $k$ is the negative binomial "clumping" parameter. "Equation (5) says that for a host pop. to be stably reg. by its parasites, it is necessary that the parasites' influence on host repro. be not too severe relativel to the parasite induced mortality (i.e. that $\beta$ be small relative to $\alpha$) and /or that the parasites be highly over dispersed ($k$ small)." However, if $k$ is too small, not equil. exists. "parasite-induced dimnuation in host repro. (increased $\beta$) is destabilizing" while both overdispersion (small $k$) and density dep. parasite mort. (whether do to reg. within the host or via host mortality itself) propotes stability. Figures 3 and 4 are good summaries of this. Model E: Within-host parasite repro. Give an example of a human protozoan parasite (Entamoeba hystolytica) which reporduces within the host's gut and produces transmission stages. Model now includes a within-host repro. rate $r$. Find three possible outcomes: (1) Host pop. cannot be regulated, (2) Host population is regulated and the equilbrium is always stable (as long as $0\leq k < \infty$) and (3) parasite drives host population, and itself, extinct. "Too large a value of $r$ produces extinction; too small a value of $r$ can (derpending on the other parameter values) lead to unregulated host population growth." Adding density dep. parasite mort (e.g. immune response) increases the range of stable regulation. Note: from a broader perspective, when comparing model behavior and natural systems we are really only interested in whether or not the system should persist (i.e. not evolve to extinction), the question of regulation is secondary. Model F: Time delay b/w prod. of transmission stage and its maturation to being able to infect a host. First form is to just incorp. a time delay $T$, second form is to explicitly model infective stages. Found time delays can have a destabilizing effect. Conclusion: Cite Park (1948) and Lancinani (1975) as examples where parasites have been shown to affect host population growth rates. Also cite a study by Anderson, Whitfield, and Mills (1977) }
}

@Article{MayAndAnderson1979,
  Title                    = {Population biology of infectious diseases: Part {II}},
  Author                   = {May, Robert M. and Anderson, Roy M.},
  Journal                  = {Nature},
  Year                     = {1979},
  Pages                    = {455-461},
  Volume                   = {280},

  Annote                   = {Extend discussion of part I to include other types of parsites and trans. processes. ng the total mort. rate due to infections across the entire host pop equal to $\alpha P$. Transform the indirect model to one of direct transmission by assuming that the free-living infect. is short lived and adjusted instantaneously to their equilibrium level for the current host and parasite densities. There's a unshown phase plane analysis and the come up with three scenarios. (a) the host pop. is reg. by the disease and the mean p burden per host is $r/ \alpha$, (b) host pop is unregulated, but parasite persists, and reduces its overall growth rate, and (c) the p. birth rate is so small that the p. cannot become est. even at infinite pop densities ($N_T$ is apparently negative b/c per parasite infect. stage prod. rate,$\lambda$ is less than rate at which infect. hosts are lost, $(\mu + b + \alpha)$. in some sense $\beta$ in part I is equivalent to $\lambda$ in part two. Mention effective net reproductive rate, but don't use term $R_0$. Indirect Lifecycles: Double the number of ODE's to describe the system. Ignoring immunity or making special assumpt. about "super infect" the system consists of four ODEs and is simliar to some classic epidemiological models: Ross-MacDonald for malaria, Nasell-Hirsch for schistosomiasis, and Dietz for arbovirus. They note, however, in these models and their great number of expansions and refinements that the terminal and intermed. host densities have previously been assumed to be constant. Since there are two hosts, apparently there is a combined threshold for both hosts. They then talk about making the system more complex by explicitly modelling trans stages as the did in the first macro model presented and how they can be reduced if the time scale of certain processes are very short relative to others (again paralleling the first part) so that you assume these short time scale events rapidly reach their eq. values. Discussions of malaria and shistosomiasis. Discuss evolutionary trends. One is how "the regulatory potential of an infectious disease will, therefore, typiclly change as time goes by." This is because the pathogen may be evolving to a more benign state and the host may be reducing its susceptiblity. Cite a number of studies where "increased pathogenicity may often be associated with enhanced rates of production (within a host) of the parasite's transmission stages" (citation numbers 38, 42, 43). Talk about general expected trends such as how host's with low densities should select for parasites which have indirect transmission where the trans. efficiency is very high (e.g. from prey to predator) or how pathogenicity and prevalence in the host should be inversely related. }
}

@Article{MayAndAnderson1983,
  Title                    = {Epidemiology and genetics in the coevolution of parasites and hosts},
  Author                   = {May, R. M. and Anderson, R. M. },
  Journal                  = {Proceedings of the Royal Society of London B, Biological Sciences},
  Year                     = {1983},
  Pages                    = {281-313},
  Volume                   = {219},

  Annote                   = {"..these models go beyond most previous stdies in that they incorporate both the tenetics and the epidemiology of parasite-host associations" Section 3 c one locus two allele model of h-p interaction discrete time reproduction occurs at end of time period derive a the term $s$ which is the probablity of a host dying from the infection (equivalent to mort. comp. of virulence) (iii) Assume population is being regulated by some other factor, therefore, total density of host is fixed b/w generations, only genotype freq. change Result: Can get polymorphisms, but can be anywhere from stable to chaotic. May goes on about chaos, cites a number of his own papers (1979, 83a) (iv) Assume population is regulated by parasite, therfore, densities fluctuate."\ldots greater propensity towards chaotic behavior" than in case where pop is reg. by other factors. "non-steady dynamical behaviour tends to arise if all $s_i$ are close to unity". (d) Overlapping generations: Assume L-V pop. reg. Hard to maintain polymorphism without a finite $K$. Multiple host and parasite genotypes. Admit very complex. Look only at a simple case then talk about a study by Beck(1983) which they say is pretty neat and elegant. Quote him as saying "a high transmission rate and low recovery and death rates are an advantage to the parasite" Section 4 "\ldots reappraise Hamilton's mathematical models in light of our more explicit description of epidemiological processes... for full details see May, Root, \& Seger (in prep)." (a) in this section they replace Hamilton's fitness function with the ones they derived earlier based on epidemiological processes but still ignore density dependence/regulation of host population size. Apparently Hamilton's models were only gene freq. models, no host density factors and had a very phenomenological freq. dep. fitness function. Looking at maximization of host geometric fitness and comparing that of a sexual host with an asex host. May and Anderson get rid of an asymmetry in the model (not clear on that one) and incorp. host density effects. Then replace Hamilton's phenomenological freq. dep. fitness function with the mechanistic gene for gene models they derive earlier in the paper. Their model appears is "asymptotically similar" to Hamiltons if you assume no host recovery from the infection. M\&A find the range at which sexual host does better than asex is severely reduced i.e. vanishes when compared with Hamilton if infection is not completely lethal (even if 99.9PERCENT lethal). (b) they now add density dependence to these models. The asex always wins, even with 100PERCENT lethal infections. (c) Expand model to include two loci Again comparing to results from Hamilton 1980 and again "these two locus systems have frequency dependence, but no density dependence" Results are similar to those in (a) where only get Hamilton's results if disease is 100PERCENT lethal. Section 5: Multiple infections and other complications}
}

@Article{MayAndNowak1995,
  Title                    = {Coinfection and the evolution of parasite virulence},
  Author                   = {May, R. M. and Nowak, M. A.},
  Journal                  = {Proceedings of the Royal Society of London B, Biological Sciences},
  Year                     = {1995},
  Pages                    = {209-215},
  Volume                   = {261},

  Abstract                 = {Analyses of the selection pressures acting on parasite virulence are made more complicated when individual hosts can simultaneously harbour many different strains or genotypes of a parasite. Here we explore the evolutionary dynamics of host-parasite associations in which individual hosts can be coinfected with many different parasite strains. (We take coinfection to mean that each strain transmits at a rate unaffected by the presence of others in the same host.) This study thus represents the opposite extreme to our earlier work on superinfection in which there is a dominance hierarchy such that only the most virulent strain present in a host is transmitted. For highly diverse populations of parasite strains, we find that such coinfection leads to selection for strains whose virulence-levels lie in a relatively narrowband close to the maximum consistent with the parasite's basic preproductive ratio, R(0), exceeding unity. }
}

