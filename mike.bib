@Article{Lynch2005,
  Title                    = {Simple evolutionary pathways to complex proteins},
  Author                   = {Lynch, M.},
  Journal                  = {Protein Science},
  Year                     = {2005},
  Pages                    = {2217-2225},
  Volume                   = {14},

  Abstract                 = {A recent paper in this journal has challenged the idea that complex adaptive features of proteins can be explained by known molecular, genetic, and evolutionary mechanisms. It is shown here that the conclusions of this prior work are an artifact of unwarranted biological assumptions, inappropriate mathematical modeling, and faulty logic. Numerous simple pathways exist by which adaptive multi-residue functions can evolve on time scales or a million years (or much less) in populations of only moderate size. Thus, the classical evolutionary trajectory of descent with modification is adequate to explain the diversification of protein functions. },
  Comment                  = {Paper copy filed.},
